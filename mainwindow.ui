/* Style de la liste déroulante */
QComboBox QAbstractItemView {
    background-color: #ffffff; /* Couleur de fond de la liste */
    border: 1px solid #7a7a7a; /* Bordure de la liste */
    selection-background-color: #0078d7; /* Couleur de fond lors de la sélection */
    selection-color: white; /* Couleur du texte lors de la sélection */
}

/* Style pour le bouton de la liste déroulante */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 25px; /* Largeur du bouton de liste déroulante */
    border-left: 1px solid #7a7a7a; /* Bordure gauche du bouton */
}

/* Style pour la flèche du bouton de liste déroulante */
QComboBox::down-arrow {
    image: url(arrow.png); /* Remplace par le chemin de ton image de flèche */
    width: 10px; /* Largeur de la flèche */
    height: 10px; /* Hauteur de la flèche */
}

/* Effet au survol */
QComboBox:hover {
    border: 2px solid #0056b3; /* Couleur de bordure au survol */
}

QComboBox:focus {
    border: 2px solid #0078d7; /* Couleur de bordure lors du focus */
}
QComboBox::down-arrow {
    color: #333; /* Couleur de la flèche */
    width: 10px; /* Largeur de la flèche */
    height: 10px; /* Hauteur de la flèche */
}</string>
